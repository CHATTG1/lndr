bind-address = "{{LNDR_BIND_ADDRESS}}"
bind-port = {{LNDR_BIND_PORT}}

lndr-ucacs {
  usd = "{{LNDR_UCAC_USD}}"
  jpy  = "{{LNDR_UCAC_JPY}}"
  krw = "{{LNDR_UCAC_KRW}}"
  aud = "{{LNDR_UCAC_AUD}}"
  cad = "{{LNDR_UCAC_CAD}}"
  chf = "{{LNDR_UCAC_CHF}}"
  cny = "{{LNDR_UCAC_CNY}}"
  dkk = "{{LNDR_UCAC_DKK}}"
  eur = "{{LNDR_UCAC_EUR}}"
  gbp = "{{LNDR_UCAC_GBP}}"
  hkd = "{{LNDR_UCAC_HKD}}"
  nok = "{{LNDR_UCAC_NOK}}"
  nzd = "{{LNDR_UCAC_NZD}}"
  sek = "{{LNDR_UCAC_SEK}}"
