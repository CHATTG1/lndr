credit-protocol-address = "{{CREDIT_PROTOCOL_ADDRESS}}"

issue-credit-event = "{{ISSUE_CREDIT_EVENT}}"

scan-start-block = {{SCAN_START_BLOCK}}

db {
  host = "{{DB_HOST}}"
  port = {{DB_PORT}}
  user = "{{DB_USER}}"
  user-password = "{{DB_PASSWORD}}"
  name = "{{DB_NAME}}"
}

execution-private-key = "{{ETHEREUM_PRIVATE_KEY}}"
gas-price = {{ETHEREUM_GAS_PRICE}}
max-gas = {{ETHEREUM_MAX_GAS}}

urban-airship {
  key = "{{URBAN_AIRSHIP_KEY}}"
  secret = "{{URBAN_AIRSHIP_SECRET}}"
}


heartbeat-interval = {{HEARTBEAT_INTERVAL}}

aws {
  photo-bucket = "{{S3_PHOTO_BUCKET}}"
  access-key-id = "{{AWS_ACCESS_KEY_ID}}"
  secret-access-key= "{{AWS_SECRET_ACCESS_KEY}}"
}

web3-url = "{{ETHEREUM_CLIENT_URL}}"
